ry = new THREE.SphereGeometry(EARTH_RADIUS, 16, 16);\r\n    const earthTexture = require(\"../assets/texture/planet/earth.png\")\r\n    const earthMaterial = new THREE.MeshPhongMaterial({\r\n        map: textureLoader.load(moonTexture),\r\n    });\r\n    earth = new THREE.Mesh(earthGeometry, earthMaterial);\r\n    scene.add(earth);\r\n\r\n    //渲染器\r\n    renderer = new THREE.WebGLRenderer({});\r\n    renderer.setPixelRatio(window.devicePixelRatio);\r\n    renderer.setSize(window.innerWidth, window.innerHeight);\r\n\r\n    //渲染器_阴影: 是 \r\n    renderer.shadowMap.enabled = true;\r\n    document.body.appendChild(renderer.domElement);\r\n\r\n    //绑定镜头控制器\r\n    const controls = new OrbitControls(camera, renderer.domElement);\r\n}\r\nfunction animate() {\r\n    //时间_自时钟启动后的秒数\r\n    const elapsed = clock.getElapsedTime();\r\n    moon.position.set(Math.sin(elapsed) * 5, 0, Math.cos(elapsed) * 5);\r\n\r\n    //自转\r\n    var axis = new THREE.Vector3(0, 1, 0);\r\n    //earth.rotateOnAxis(axis, elapsed * Math.PI / 1000);\r\n    renderer.render(scene, camera);\r\n    requestAnimationFrame(animate);\r\n}\r\ninit();\r\nanimate();"},"lineCount":null}},"error":null,"hash":"10a024f85a5ddc768f4c5d3764c38f86","cacheData":{"env":{}}}